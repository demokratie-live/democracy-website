    "@tanstack/react-query": "^5.90.12",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "jsonwebtoken": "^9.0.3",
    "next": "16.1.0",
    "nodemailer": "^7.0.11",
    "payload": "^3.69.0",
    "prisma": "5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "sharp": "^0.34.5",
